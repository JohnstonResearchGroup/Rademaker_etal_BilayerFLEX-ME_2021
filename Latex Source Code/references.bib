	Doi = {10.1038/s41467-019-08560-z},
	Id = {Song2019},
	Isbn = {2041-1723},
	Journal = {Nature Communications},
	Number = {1},
	Pages = {758},
	Title = {Evidence of cooperative effect on the enhanced superconducting transition temperature at the \protect{FeSe/SrTiO$_3$} interface},
	Ty = {JOUR},
	Url = {https://doi.org/10.1038/s41467-019-08560-z},
	Volume = {10},
	Year = {2019},
