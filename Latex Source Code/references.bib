	Journal = {Physica C: Superconductivity},
	Keywords = {High-, Small-q, Pseudogap},
	Note = {Proceedings of the 8th International Conference on Materials and Mechanisms of Superconductivity and High Temperature Superconductors},
	Pages = {1125 - 1126},
	Title = {Small-q electron--phonon interaction and the phase diagram of the cuprates},
	Url = {http://www.sciencedirect.com/science/article/pii/S0921453407004169},
	Volume = {460-462},
	Year = {2007},
